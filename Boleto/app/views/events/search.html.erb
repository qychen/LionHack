<p id="notice"><%= notice %></p> 
<h1 class='fuckyou'>Search Result</h1>
<table class='table'>
  <thead>
    <tr>
      <th> Event </th>
      <th> Status </th>
      <th> Title </th>
      <th> Description </th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @search_result.each do |event| %>
      <tr>
        <td><%= event.event_id %></td>
        <td><%= event.status %></td>
        <td><%= event.title %></td>
        <td><%= event.description %></td>
        <td><%= link_to 'Details', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<footer style='margin-top: 16.5cm; margin-left: 0cm'></footer>